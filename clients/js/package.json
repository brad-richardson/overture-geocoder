{
  "name": "overture-geocoder",
  "version": "0.1.0",
  "description": "Forward geocoder using Overture Maps data with Nominatim-compatible API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "test": "vitest",
    "test:ci": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "geocoding",
    "overture",
    "maps",
    "nominatim"
  ],
  "author": "Brad Richardson",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  },
  "optionalDependencies": {
    "@duckdb/duckdb-wasm": "^1.28.0"
  }
}
