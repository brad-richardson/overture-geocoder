{
  "name": "@bradrichardson/overture-geocoder",
  "version": "0.2.1",
  "description": "Forward geocoder using Overture Maps data with Nominatim-compatible API",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "test:ci": "vitest run",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "geocoding",
    "overture",
    "maps",
    "nominatim",
    "places",
    "addresses"
  ],
  "author": "Brad Richardson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brad-richardson/overture-geocoder.git",
    "directory": "clients/js"
  },
  "homepage": "https://github.com/brad-richardson/overture-geocoder#readme",
  "bugs": {
    "url": "https://github.com/brad-richardson/overture-geocoder/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@bradrichardson/overturemaps": "^0.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.5.1",
    "typescript": "^5.3.3",
    "vitest": "^1.0.0"
  }
}
